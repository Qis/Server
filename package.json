{
  "name": "server",
  "version": "1.0.0",
  "description": "C++ server example.",
  "main": "index.html",
  "scripts": {
    "watch": "parcel watch --public-url ./ --cache-dir build/bundle/cache --out-dir build/bundle/debug html/index.html html/favicon.ico",
    "build": "parcel build --public-url ./ --cache-dir build/bundle/cache --out-dir build/bundle/release html/index.html html/favicon.ico"
  },
  "browserslist": [
    "last 2 edge versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 opera versions",
    "last 3 safari versions",
    "last 3 samsung versions",
    "last 3 and_chr versions",
    "last 3 and_ff versions",
    "last 3 ios_saf versions"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "grid": true
      }
    }
  },
  "alias": {
    "src": "./src"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "browserslist": "^4.8.6",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "postcss-modules": "^1.5.0",
    "svelte": "^3.18.1"
  },
  "keywords": [],
  "license": "Boost"
}
